ncdisp('projected_ice_26.nc')

model=ncread('projected_ice_26.nc', 'sic');
lat=ncread('projected_ice_26.nc', 'lat');
lon=ncread('projected_ice_26.nc', 'lon');
time= ncread('projected_ice_26.nc', 'time');

timeyear= time/365
%% 
sep = NaN([size(lat, 1) size(lon, 1) 93]);
svector= 9:12:1140 ;
sep= model(:,:,svector);
%average all the marchs
meansep=mean(sep,[1 2],'omitnan');
meansep1=squeeze(meansep);

%% IGNORE ABOVE working with new CSV
filename = 'Hist_2.6model8.5.csv'
file = csvread(filename)
disp(file)
%for i = 1:size(file,1)
    %file2(:,i,:) = file(:,i,:)
%end
a = file(:,1)
b = file(:,2)
c = file(:,3)

for i = 1:size(a)
    a(i) = (a(i)-14.5953495901526)/abs(c(i)-2006)
    b(i) = (b(i)-14.9729071978214)/abs(c(i)-2006)
end
